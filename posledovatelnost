namespace Последовательность
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int progressionValue = 5;
            int progressionStep = 7;
            int maxValue = 96;

            //while (progressionValue <= maxValue)
            //{
            //    Console.WriteLine(progressionValue);
            //    progressionValue += progressionStep;
            //}

            for (int i = 0; i <= maxValue / progressionStep; i++)
            {
                Console.WriteLine(progressionValue);
                progressionValue += progressionStep;
            }

            Console.ReadKey();
        }
    }
}
